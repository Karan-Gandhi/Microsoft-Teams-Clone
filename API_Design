API:
    - Team:
        id
        members
        Name
        Messages
        activeMeetings
        admins
        feed (consisting of messages and meetings)

        - Message:
            teamid
            Message
            fromWho
            timeCreated

        - Meeting: (will be notified on the team)
            id
            title
            teamid
            start time
            duration
            reoccuring?
            timeCreated
            isStarted
            participants

    - User:
        id
        Name
        email
        password
        teams

routes:
    getTeamFromID: GET
    getTeamJoinLink: GET
